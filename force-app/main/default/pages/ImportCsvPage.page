<apex:page controller="ImportCsvController">
    <apex:form >
        <apex:pagemessages />
        <apex:pageBlock >
            <apex:pageBlockSection columns="4"> 
                <apex:inputFile value="{!csvFileBody}"  filename="{!csvAsString}"/>
                <apex:commandButton value="Import Contact" action="{!importCSVFile}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
        <apex:pageBlock >
            <apex:pageblocktable value="{!conlist}" var="con">
                <apex:column value="{!con.Lastname}" />
                <apex:column value="{!con.Title}" />
                <apex:column value="{!con.Birthdate}" />
                <apex:column value="{!con.Import_Date_Time__c}" />
                <apex:column value="{!con.Import_Boolean_Type__c}" />
                <apex:column value="{!con.Import_Text_Area__c}" />
            </apex:pageblocktable>
        </apex:pageBlock>
    </apex:form>
</apex:page>